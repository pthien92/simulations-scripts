			destName   = "UE3"
			tftId      = "3"     
		/>
		<filter
			destName   = "UE4"
			tftId      = "4"     
		/>
		<filter
			destName   = "UE5"
			tftId      = "5"     
		/>
		<filter
			destName   = "UE6"
			tftId      = "6"     
		/>
		<filter
			destName   = "UE7"
			tftId      = "7"     
		/>
		<filter
			destName   = "UE8"
			tftId      = "8"     
		/>
	</filterTable>
</config>